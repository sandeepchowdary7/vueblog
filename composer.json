{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "bensampo/laravel-enum": "^1.14",
        "beyondcode/dusk-dashboard": "^1.0",
        "beyondcode/laravel-self-diagnosis": "^1.0",
        "binarytorch/larecipe": "^1.2",
        "chelout/laravel-relationship-events": "^0.6.1",
        "fideloper/proxy": "^4.0",
        "freshbitsweb/laratables": "^1.0",
        "fzaninotto/faker": "^1.8",
        "khill/datatableplus": "dev-master",
        "khill/lavacharts": "^3.1",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.1",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "marvinlabs/laravel-luhn": "^1.0",
        "protoqol/prequel": "^0.6.2@beta",
        "rennokki/befriended": "^1.2",
        "spatie/laravel-view-models": "^1.0",
        "wildside/userstamps": "^0.5.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "beyondcode/laravel-er-diagram-generator": "^1.2",
        "filp/whoops": "^2.0",
        "laravel/dusk": "^4.0",
        "laravel/telescope": "^0.1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "files": [
            "app/helpers.php",
            "app/errors.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
